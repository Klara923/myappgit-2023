<!-- Main -->

   <link rel="stylesheet" type="text/css" href="/css/my-styles-05.css">
<main id="main column col-12">
  <div id="cv">
    <div class="container cv-container">
      <!-- Header/Hero section -->
      <section class="header-section columns col-md-12">
        <div class="photo-container column col-3 col-md-12">
          <img
            src="img/profile-picture-png-compressed.png"
            alt="Profile image"
          />
        </div>
        <div class="desc-container column col-6 col-md-12">
          <div class="about-me-container column col-12">
            <div class="name-container column col-12">
              <span class="nmd column col-12">NMD Student</span>
              <div class="names-container">
                <h1 class="first-name">
                  Klara <br /><span class="last-name">Swiecicka</span>
                </h1>
              </div>
            </div>
          </div>
          <div class="about-me-desc column col-12 col-md-12">
            <p class="about-me column col-12 col-md-12">
              I'm an international student from Poland, currently in my second
              year of the New Media Design program at Jönköping University. I'm
              actively seeking part-time employment to further develop my skills
              while ensuring financial stability throughout my studies. I'm a
              quick learner, creative, and thrive in collaborative team
              environments. I have extensive professional experience spanning,
              both Poland and Sweden.
            </p>
          </div>
        </div>
      </section>
      <!-- Main section -->
      <main class="main-section columns col-12 col-md-12">
        <!-- Left column -->
        <div class="left-column column col-4 col-md-12">
          <div class="left-column-container columns">
            <div class="languages-container skills-container columns col-12">
              <h2 class="column col-3">Languages</h2>
              <div class="language columns col-oneline col-12">
                <p class="column col-4">Polish</p>
                <div class="dots-container columns col-oneline col-8">
                  <span class="dot filled column col-2"></span>
                  <span class="dot filled column col-2"></span>
                  <span class="dot filled column col-2"></span>
                  <span class="dot filled column col-2"></span>
                  <span class="dot filled column col-2"></span>
                </div>
              </div>
              <div class="language columns col-oneline col-12">
                <p class="column col-4">English</p>
                <div class="dots-container columns col-oneline col-8">
                  <span class="dot filled column col-2"></span>
                  <span class="dot filled column col-2"></span>
                  <span class="dot filled column col-2"></span>
                  <span class="dot filled column col-2"></span>
                  <span class="dot column col-2"></span>
                </div>
              </div>
              <div class="language columns col-oneline col-12">
                <p class="column col-4">Swedish</p>
                <div class="dots-container columns col-oneline col-8">
                  <span class="dot filled column col-2"></span>
                  <span class="dot column col-2"></span>
                  <span class="dot column col-2"></span>
                  <span class="dot column col-2"></span>
                  <span class="dot column col-2"></span>
                </div>
              </div>
            </div>
            <div class="tech-skills-container skills-container columns col-12">
              <h2 class="column col-12">Technical Skills</h2>
              <p class="column col-12">design</p>
              <div class="design-skills columns col-12">
                <div class="columns col-oneline col-12">
                  <img
                    src="img/ps.svg"
                    alt="Photoshop icon"
                    class="column col-6"
                  />
                  <img
                    src="img/ai.svg"
                    alt="Illustrator icon"
                    class="column col-6"
                  />
                </div>
                <div class="columns col-oneline col-12">
                  <img
                    src="img/id.svg"
                    alt="Indesign icon"
                    class="column col-6"
                  />
                  <img
                    src="img/figma.svg"
                    alt="Figma icon"
                    class="column col-6"
                  />
                </div>
              </div>
              <p class="column col-12">programming</p>
              <div class="prog-skills columns col-12">
                <div class="columns col-oneline col-12">
                  <img
                    class="html-icon column col-6"
                    src="img/html.svg"
                    alt=""
                  />
                  <img class="column col-6" src="img/css.svg" alt="" />
                </div>
                <div class="columns col-oneline col-12">
                  <img class="column col-6" src="img/js.svg" alt="" />
                  <img class="column col-6" src="img/react.svg" alt="" />
                </div>
              </div>
            </div>
            <div class="oth-skills skills-container columns col-12">
              <h2 class="column col-12">Other Skills</h2>
              <ul class="column col-12">
                <li><span>Creativity</span></li>
                <li><span>Team work</span></li>
                <li><span>Adabtability</span></li>
                <li><span>Thrive under time pressure</span></li>
              </ul>
            </div>
          </div>
        </div>
        <!-- Right column -->
        <div class="right-column column col-7 col-md-12">
          <div class="columns">
            <div class="edu column col-12">
              <h2>Education</h2>
              <div class="border-bottom"></div>
              <ul>
                <li>
                  <span>2022-present</span> Jönköping University, School of
                  Engineering, New Media Design Program, 180 credits
                </li>
                <li>
                  <span>2021-2022</span> VIII Highschool in Gdansk, Humanistic
                  Program
                </li>
                <li>
                  <span>2020-2021</span> Highschool of Arts in Gdynia,
                  Photographic Program
                </li>
              </ul>
            </div>
            <div class="work-exp column col-12">
              <h2>Work Experience</h2>
              <div class="border-bottom"></div>
              <ul>
                {{#each works}}
                <li><span>{{wDate}}</span> {{wTitle}}, {{wPlace}}, {{wCountry}}</li>
                {{/each}}
              </ul>
            </div>
            <div class="oth-exp column col-12">
              <h2>Other Experience</h2>
              <div class="border-bottom"></div>
              <ul>
                <li>Volunteering and scouting</li>
                <li>Gdynia Buisness Week 2019</li>
                <li>Sailing courses</li>
                <li>Graphic Agent in JU association - Justice</li>
              </ul>
              <div class="border-bottom"></div>
            </div>
          </div>
        </div>
      </main>
</main>